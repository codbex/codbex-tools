{
    "name": "@codbex/aws-cdk-monorepo",
    "version": "0.1.3",
    "private": true,
    "scripts": {
        "login": "pnpm login --scope=@codbex --auth-type=legacy --registry=https://npm.pkg.github.com",
        "build": "pnpm install && turbo run build",
        "test": "pnpm install && turbo run test",
        "cdk:synth": "pnpm install && turbo run cdk:synth",
        "cdk:deploy-apps": "pnpm install && turbo run cdk:deploy-apps",
        "cdk:deploy-core": "pnpm install && turbo run cdk:deploy-core",
        "cdk:destroy": "turbo run cdk:destroy",
        "lint": "turbo run lint",
        "lint:fix": "turbo run lint:fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html,yaml}\" --config ./.prettierrc",
        "format-check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html,yaml}\" --config ./.prettierrc"
    },
    "devDependencies": {
        "@codbex/eslint-config-aws-cdk": "workspace:*",
        "@types/jest": "29.5.12",
        "@types/node": "22.7.4",
        "esbuild": "0.21.3",
        "husky": "^9.0.11",
        "jest": "29.7.0",
        "lint-staged": "^15.2.7",
        "ts-jest": "29.2.5",
        "turbo": "1.13.3",
        "typescript": "5.6.2"
    },
    "packageManager": "pnpm@9.1.2"
}